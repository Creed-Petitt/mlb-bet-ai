    "import sqlite3\n",
    "\n",
    "conn = sqlite3.connect(\"data/mlb.db\")\n",
    "c = conn.cursor()\n",
    "\n",
    "# Add new column if it doesn't exist\n",
    "try:\n",
    "    c.execute(\"ALTER TABLE hit_props ADD COLUMN american_odds TEXT\")\n",
    "except sqlite3.OperationalError:\n",
    "    print(\"Column already exists.\")\n",
    "\n",
    "conn.commit()\n",
    "conn.close()"
   ]
